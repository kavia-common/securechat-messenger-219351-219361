#!/usr/bin/env bash
# Stub Gradle wrapper for CI environments that wrongly invoke ./gradlew on an Expo-managed app.
# This is a no-op to prevent failures like: bash: line 1: ./gradlew: No such file or directory
# If you actually need native builds, run: npm run prebuild:android (which will generate a real gradlew)
echo "[stub-gradlew] Expo-managed app: native Gradle build is skipped."
echo "[stub-gradlew] Command received: $*"
exit 0
